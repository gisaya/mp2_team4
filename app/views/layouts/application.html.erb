<!DOCTYPE html>
<html>
<head>
  <title>Timetable</title>
	<meta charset="utf-8">

	<%# stylesheet_link_tag "stylesheets/home.css.scss", media: "all" %>

  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
	<!-- Navigator -->
	<%= render "layouts/header" %>

	<!-- Flash -->
	<div id="flash" style="width: 100%; position: fixed; top: 0;">
		<%= render "layouts/flash" %>
	</div>

	<%= yield %>

</body>
</html>
